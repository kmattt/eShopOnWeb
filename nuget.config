<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear /> <!-- Clears all existing package sources -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" /> <!-- Adds the default NuGet package source -->
  </packageSources>
  <config>
    <add key="globalPackagesFolder" value="packages" /> <!-- Sets the global packages folder -->
  </config>
</configuration>
